{
  "name": "abstract-data-manager",
  "version": "1.0.2",
  "description": "An abstract manager which can be useful for broadcasting lazily-loaded data.",
  "author": "Will Tesler",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wtesler/abstract-data-manager.git"
  },
  "homepage": "https://github.com/wtesler/abstract-data-manager",
  "scripts": {
    "build": "npm run clean && babel src/lib --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "dependencies": {},
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "rimraf": "3.0.2"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": 3
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "keywords": [
    "remote",
    "broadcast",
    "manager"
  ]
}
